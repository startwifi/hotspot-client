{
  "extends": [
    "airbnb",
    "prettier/react"
  ],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "semi": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "react/jsx-filename-extension": [1, {"extensions": [".js"]}],
    "prettier/prettier": "error"
  }
}
